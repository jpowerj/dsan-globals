  file = {C\:\\Users\\jpjac\\Zotero\\storage\\VSUDXZGY\\Zanettin - 2016 - ‘The deadliest error’ translation, international .pdf;C\:\\Users\\jpjac\\Zotero\\storage\\6C4A2ZKM\\13556509.2016.html}
}

