  keywords = {History / General,Philosophy / Political,Political Science / History \& Theory,Social Science / General,Social Science / Sociology / General}
}

@book{anderson_private_2017,
  title = {Private {{Government}}: {{How Employers Rule Our Lives}} (and {{Why We Don}}'t {{Talk}} about {{It}})},
  shorttitle = {Private {{Government}}},
  author = {Anderson, Elizabeth},
  year = {2017},
  month = may,
  publisher = {{Princeton University Press}},
  abstract = {Why our workplaces are authoritarian private governments\textemdash and why we can't see itOne in four American workers says their workplace is a "dictatorship." Yet that number probably would be even higher if we recognized most employers for what they are\textemdash private governments with sweeping authoritarian power over our lives, on duty and off. We normally think of government as something only the state does, yet many of us are governed far more\textemdash and far more obtrusively\textemdash by the private government of the workplace. In this provocative and compelling book, Elizabeth Anderson argues that the failure to see this stems from long-standing confusions. These confusions explain why, despite all evidence to the contrary, we still talk as if free markets make workers free\textemdash and why so many employers advocate less government even while they act as dictators in their businesses.In many workplaces, employers minutely regulate workers' speech, clothing, and manners, leaving them with little privacy and few other rights. And employers often extend their authority to workers' off-duty lives. Workers can be fired for their political speech, recreational activities, diet, and almost anything else employers care to govern. Yet we continue to talk as if early advocates of market society\textemdash from John Locke and Adam Smith to Thomas Paine and Abraham Lincoln\textemdash were right when they argued that it would free workers from oppressive authorities. That dream was shattered by the Industrial Revolution, but the myth endures.Private Government offers a better way to talk about the workplace, opening up space for discovering how workers can enjoy real freedom.Based on the prestigious Tanner Lectures delivered at Princeton University's Center for Human Values, Private Government is edited and introduced by Stephen Macedo and includes commentary by cultural critic David Bromwich, economist Tyler Cowen, historian Ann Hughes, and philosopher Niko Kolodny.},
  googlebooks = {JR5VDgAAQBAJ},
  isbn = {978-1-4008-8778-1},
  langid = {english},
  keywords = {Business \& Economics / Economics / General,Law / Labor \& Employment,Philosophy / Political,Political Science / History \& Theory,Political Science / Labor \& Industrial Relations}
}

