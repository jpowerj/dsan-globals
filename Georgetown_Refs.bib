@article{bowles_revenge_1993,
  title = {The {{Revenge}} of {{Homo Economicus}}: {{Contested Exchange}} and the {{Revival}} of {{Political Economy}}},
  shorttitle = {The {{Revenge}} of {{Homo Economicus}}},
  author = {Bowles, Samuel and Gintis, Herbert},
